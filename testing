local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Exile's Hoopz Script", HidePremium = false, SaveConfig = true, ConfigFolder = "exile#7482"})

local mainTab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

mainTab:AddButton({
	Name = "Aimlock (USE CENTERED SHIFTLOCK)",
	Callback = function()
        loadstring(game:HttpGet('https://github.com/Rebelisamazing/msahub/blob/main/private'))()	
  	end    
})

mainTab:AddSlider({
	Name = "Walkspeed",
	Min = 0,
	Max = 20,
	Default = 5,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "WS",
	Callback = function(s)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
	end    
})




end
OrionLib:Init()
